 ﻿<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vm="clr-namespace:PIM_WPF.ViewModel"
                     xmlns:view="clr-namespace:PIM_WPF.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:IntroducaoVM}">
        <view:Introducao />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MaisVM}">
        <view:Mais />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ObrasVM}">
        <view:Obras />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:QuestionarioVM}">
        <view:Questionario />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ResultadosVM}">
        <view:Resultados />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SobreVM}">
        <view:Sobre />
    </DataTemplate>

</ResourceDictionary>